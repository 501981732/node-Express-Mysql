<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <title></title>
    <link rel="stylesheet" href="../css/bootstrap/dist/css/bootstrap.min.css">
    <link rel="stylesheet" href="../css/reset.css">
</head>
<body>
    <div class="container">
        <div class="row clearfix">
            <div class="col-md-12 column">
            <% include components/header.ejs %>
            <div class="top_wrap">
                我是管理首页
                <button type="submit" class="btn btn-default" id="btn">退出登录</button>
            </div>
        </div>
    </div>
    <script src='../js/ajax.js'></script>
<script>
    (function(){
        var btn = document.querySelector('#btn')
        console.log(btn)
        btn.onclick = function() {
            ajax({
                url: '/admin/out',
                data: {},
                type: 'get',
                success: function(str) {
                    // let json = eval('('+str+')')
                    // console.log(json)
                    if(str.code =1) {
                        location.href = '/admin'
                    }
                },
                error: function() {
                    alert('通讯错误')
                }
            })
            }
    })()
</script>
</body>

</html>